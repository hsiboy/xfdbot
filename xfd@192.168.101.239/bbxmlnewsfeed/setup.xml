<alphasign>

  <memoryConfig>
  <!-- cctray feed -->
    <textConfig label="P" size="512" start="always" stop="never"/>
    <!-- BBC NEWS -->
    <textConfig label="B" size="512" start="12:00" stop="13:00"/>
    <textConfig label="C" size="512" start="12:00" stop="13:00"/>
    <!-- Time -->
    <textConfig label="T" size="64" start="always" stop="never"/>
    <!-- Weather -->
    <textConfig label="W" size="64" start="12:00" stop="13:00"/>
    
    <!-- images -->
    <dotsConfig label="A" height="7" width="18" colors="1"/>
    <dotsConfig label="B" height="7" width="18" colors="1"/>
    <dotsConfig label="C" height="14" width="14" colors="3"/>
    <dotsConfig label="P" height="7" width="34" colors="3"/>
    <dotsConfig label="D" height="16" width="16" colors="3"/>
    <dotsConfig label="E" height="9" width="26" colors="3"/>
    <dotsConfig label="R" height="8" width="7" colors="1"/>
    <dotsConfig label="G" height="8" width="7" colors="1"/>
    <dotsConfig label="Y" height="8" width="7" colors="1"/>
     
    <stringConfig label="P" size="512"/>
    <stringConfig label="B" size="512"/>
    <stringConfig label="C" size="512"/>
    <stringConfig label="W" size="512"/>
  </memoryConfig>

<timeFormat format="24-hour-blinking-colon"/>

<sequence labels="PBPCPTPW" mode="useTimeSchedule"/>
<!-- <sequence labels="P"/> -->

  <text label="P"><callString label="P"/><mode display="starburst"/></text>
  <text label="B"><green/><fullHeightFancy/><mode display="rotate"/><speed5/><callString label="B"/><mode display="starburst"/></text>
  <text label="C"><green/><fullHeightFancy/><mode display="rotate"/><speed5/><callString label="C"/><mode display="starburst"/></text>
  <text label="T"><orange/><fullHeightFancy/><mode display="hold"/><time/><mode display="starburst"/></text>
  <text label="W"><green/><fullHeightFancy/><mode display="rotate"/><speed5/><callString label="W"/><callDots label="A"/><mode display="starburst"/></text>
  


